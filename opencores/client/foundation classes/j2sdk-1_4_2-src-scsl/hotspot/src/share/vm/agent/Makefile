jdkHome := $(shell dirname $(shell which java))/..
isUnix := $(shell test -r c:/; echo $$?)

ifeq "$(isUnix)" "1"
    CPS := :
else
    CPS := ";"
endif


ALLFILES := $(FILELIST) $(shell ls sun/jvm/hotspot/jdi/*.java)

# tools.jar is needed by the JDI - SA binding
classpath = lib/maf-1_0.jar$(CPS)$(jdkHome)/lib/tools.jar

# FIXME: autogenerate call to rmic
# Tagging it on because there's no reason not to run it
all: $(ALLFILES)
	@mkdir -p classes
	@javac -classpath $(classpath) -deprecation -g -d classes $(ALLFILES)
	@rmic -classpath classes -d classes sun.jvm.hotspot.debugger.remote.RemoteDebuggerServer

allprof: $(ALLFILES)
	@mkdir -p classes
	@javac -J-Xprof -classpath $(classpath) -deprecation -g -d classes $(ALLFILES)
	@rmic -classpath classes -d classes sun.jvm.hotspot.debugger.remote.RemoteDebuggerServer


.PHONY: natives
natives:
	cd ../../../os/`java -classpath classes sun.jvm.hotspot.utilities.PlatformInfo`/agent; $(MAKE) all

docs:
	@javadoc -d doc $(PKGLIST)

sizes: $(ALLFILES)
	wc -l $(ALLFILES)

cscope: $(ALLFILES)
	echo $(ALLFILES) > java.files
	cscope -b -i java.files -f java.out 

.PHONY: sa.jar
sa.jar:
	rm -f sa.jar
	mkdir -p classes/sun/jvm/hotspot/ui/resources
	rm -f classes/sun/jvm/hotspot/ui/resources/*
	cp sun/jvm/hotspot/ui/resources/*.png classes/sun/jvm/hotspot/ui/resources/
	cd classes ; jar cvf ../sa.jar sun

clean::
	cd ../../../os/`java -classpath classes sun.jvm.hotspot.utilities.PlatformInfo`/agent; $(MAKE) clean
	rm -rf classes/*
